#!/bin/sh

# Cleanup script for OptimalBinningWoE package
# Removes compiled objects and strips debug symbols

# Remove object files
rm -f src/*.o src/*.so src/*.dll

# Remove RcppExports
rm -f src/RcppExports.o

# Strip symbols from compiled libraries (if they exist)
if [ -f "src/OptimalBinningWoE.so" ]; then
  strip --strip-debug src/OptimalBinningWoE.so 2>/dev/null || true
fi

if [ -f "libs/OptimalBinningWoE.so" ]; then
  strip --strip-debug libs/OptimalBinningWoE.so 2>/dev/null || true
fi

# Clean up auto-generated files
rm -rf src-i386/ src-x64/
